    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@uploadthing/react": "^7.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.41.0",
