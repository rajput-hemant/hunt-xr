    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "server-only": "^0.0.1",
    "sonner": "^2.0.1",
    "sst": "ion",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.174.0",
    "twilio": "^5.5.1",
    "uploadthing": "^7.5.2",
    "zod": "^3.24.2"
